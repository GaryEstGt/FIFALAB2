@model IEnumerable<Lab2FIFA.Models.Entero>

@{
    ViewBag.Title = "IndexEntero";
}

<h2>IndexEntero</h2>

<p>
    @Html.ActionLink("Create New", "CrearPorArchivo")
</p>
@using (Html.BeginForm("IndexEntero", "Pais", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="submit" name="submitButton" value="PreOrden" />
    <input type="submit" name="submitButton" value="InOrden" />
    <input type="submit" name="submitButton" value="PostOrden" />
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.valor)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.valor)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
        </td>
    </tr>
}

</table>
